CREATE MEMORY TABLE COUNCIL_MEMBERS(MEMBERCONTACT VARCHAR(255),MEMBERNAME VARCHAR(255),MEMBERPOSITION VARCHAR(255),MEMBERID INTEGER NOT NULL PRIMARY KEY,COUNCIL_ID INTEGER,CONSTRAINT FK17C9557BD4BEB913 FOREIGN KEY(MEMBERID) REFERENCES BASEIMAGESSUPPORT(ID))
