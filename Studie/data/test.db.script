CREATE MEMORY TABLE STUDIES_TABLE(ACCREDITACION VARCHAR(3) NOT NULL,ADRESS VARCHAR(100) NOT NULL,CITY VARCHAR(50) NOT NULL,COUNTOFCANDIDATES INTEGER NOT NULL,COUNTOFPROFESSORS INTEGER NOT NULL,COUNTOFSTUDENTS INTEGER NOT NULL,COUNTOFTEACHERS INTEGER NOT NULL,ENROLLEES VARCHAR(10000) NOT NULL,FORMOFTRAINING VARCHAR(100) NOT NULL,FREETRAINIG BOOLEAN NOT NULL,HOSTEL BOOLEAN NOT NULL,MILITARYDEPARTMENT BOOLEAN NOT NULL,ONGRADUATION VARCHAR(100) NOT NULL,PAIDTRAINIG BOOLEAN NOT NULL,PHONE VARCHAR(100) NOT NULL,PHONEADMISSIONS VARCHAR(100) NOT NULL,POSTGRADUATEANDDOCTORALSTUDIES BOOLEAN NOT NULL,POSTGRADUATEEDUCATION BOOLEAN NOT NULL,PREPARATORYDEPARTMENT BOOLEAN NOT NULL,QUALIFICATIONLEVEL VARCHAR(100) NOT NULL,STATUS VARCHAR(100) NOT NULL,TEXT VARCHAR(10000) NOT NULL,TITLE VARCHAR(100) NOT NULL,WEBSITE VARCHAR(100) NOT NULL,YEARS INTEGER NOT NULL,ID INTEGER NOT NULL PRIMARY KEY,CONSTRAINT FK529235B6FB746CF9 FOREIGN KEY(ID) REFERENCES BASEIMAGESSUPPORT(ID))
