<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN"
        "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">
<routes>
    <route>
        <pattern>/add</pattern>
        <implicit-parameter name="mode">add</implicit-parameter>
    </route>
    <route>
        <pattern>/delImage/{imageId:\d+}</pattern>
        <implicit-parameter name="mode">delImage</implicit-parameter>
    </route>
    <route>
        <pattern>/</pattern>
        <implicit-parameter name="mode">viewAll</implicit-parameter>
    </route>
    <route>
        <pattern>/delete/{currentPage:\d+}/{orgsID:\d+}</pattern>
        <implicit-parameter name="mode">delete</implicit-parameter>
    </route>
    <route>
        <pattern>/delete/{orgsId:\d+}</pattern>
        <implicit-parameter name="mode">delete</implicit-parameter>
    </route>
    <route>
        <pattern>/home</pattern>
        <implicit-parameter name="nAction">home</implicit-parameter>
    </route>
    <route>
        <pattern>/edit/{orgsID:\d+}</pattern>
        <implicit-parameter name="mode">edit</implicit-parameter>
    </route>
    <route>
        <pattern>/pagination/{direction}/{currentPage:\d+}</pattern>
    </route>
    <route>
        <pattern>/view/{currentPage:\d+}/{orgsId:\d+}</pattern>
    </route>
    <route>
        <pattern>/home/{currentPage:\d+}</pattern>
    </route>
    

</routes>