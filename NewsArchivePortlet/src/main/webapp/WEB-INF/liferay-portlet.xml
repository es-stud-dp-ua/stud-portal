<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN"
        "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
    <portlet>
        <portlet-name>NewsArchive</portlet-name>
        <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper
        </friendly-url-mapper-class>
        <friendly-url-mapping>NewsArchive</friendly-url-mapping>
        <friendly-url-routes>/routes.xml</friendly-url-routes>
        <instanceable>false</instanceable>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <header-portlet-javascript>/js/placeholder.min.js</header-portlet-javascript>
    </portlet>
</liferay-portlet-app>